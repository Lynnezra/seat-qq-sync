{
    "name": "Lynnezra/seat-qq-sync",
    "description": "SeAT plugin for QQ group member synchronization",
    "type": "seat-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynnezra",
            "email": "kingwangfei11@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "eveseat/services": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Lynnezra\\Seat\\QQSync\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lynnezra\\Seat\\QQSync\\QQSyncServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}